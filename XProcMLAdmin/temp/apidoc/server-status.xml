<xs:schema targetNamespace="http://marklogic.com/xdmp/status/server" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema XMLSchema.xsd" attributeFormDefault="unqualified" elementFormDefault="unqualified" xmlns="http://marklogic.com/xdmp/status/server" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:ns2="http://marklogic.com/xdmp/status/request" xmlns:ns="http://marklogic.com/xdmp/status/server" xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty" xmlns:admin="http://marklogic.com/xdmp/admin">
  <xs:simpleType name="server-id">
    <xs:annotation>
      <xs:documentation>A server id.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="server-id" type="server-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The server id.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="host-id">
    <xs:annotation>
      <xs:documentation>A host unique id (64-bit value).</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="host-id" type="host-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The unique key of the host</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="server-status" type="server-status" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The status of a server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="server-name">
    <xs:annotation>
      <xs:documentation>A server name.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:minLength value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="server-name" type="server-name" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The server name.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="server-kind">
    <xs:annotation>
      <xs:documentation>A kind of server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="http-server"/>
      <xs:enumeration value="xdbc-server"/>
      <xs:enumeration value="webDAV-server"/>
      <xs:enumeration value="task-server"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="server-kind" type="server-kind" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The kind of server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="request-kind">
    <xs:annotation>
      <xs:documentation>A kind of request</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="eval"/>
      <xs:enumeration value="invoke"/>
      <xs:enumeration value="get"/>
      <xs:enumeration value="copy"/>
      <xs:enumeration value="delete"/>
      <xs:enumeration value="lock"/>
      <xs:enumeration value="mkcol"/>
      <xs:enumeration value="move"/>
      <xs:enumeration value="options"/>
      <xs:enumeration value="propfind"/>
      <xs:enumeration value="proppatch"/>
      <xs:enumeration value="put"/>
      <xs:enumeration value="unlock"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-kind" type="request-kind" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The kind of request (for example, invoke, eval).</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="port">
    <xs:annotation>
      <xs:documentation>An integer internet port number, min 1, max 65534.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" fixed="false"/>
      <xs:maxInclusive value="65534" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="port" type="port" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The server socket bind internet port number.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="directory-pathname">
    <xs:annotation>
      <xs:documentation>It can be absolute or relative.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="root" type="directory-pathname" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The root document directory pathname.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="database-id">
    <xs:annotation>
      <xs:documentation>A database ID.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="modules" type="database-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The ID of the database that contains application modules. If on the file-system, then 0.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="database" type="database-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The ID of the content database.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="last-login" type="database-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The ID of the last login database.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="display-last-login" type="boolean" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Whether an appserver should display last-login information.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="address">
    <xs:annotation>
      <xs:documentation>A numeric internet address.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:pattern value="[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="address" type="address" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The server socket bind numeric internet address.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="backlog">
    <xs:annotation>
      <xs:documentation>An integer number of connections, min 1, max 4096.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" fixed="false"/>
      <xs:maxInclusive value="4096" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="backlog" type="backlog" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The socket listen backlog.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="threadCount">
    <xs:annotation>
      <xs:documentation>An integer number of threads, min 0, max 256.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" fixed="false"/>
      <xs:maxInclusive value="256" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="threads" type="threadCount" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The maximum number of server threads allowed.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="max-threads" type="threadCount" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The maximum number of server threads allowed.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="timeout">
    <xs:annotation>
      <xs:documentation>An integer number of seconds, min 0, max 4294967295.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" fixed="false"/>
      <xs:maxInclusive value="4294967295" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-timeout" type="timeout" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The request socket recv timeout, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="keep-alive-timeout" type="timeout" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The keep-alive socket recv timeout, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="session-timeout" type="timeout" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The session expiration timeout, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="static-expires" type="timeout" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The static content expiration interval, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="max-time-limit" type="timeout" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The upper bound for a request's time limit, in seconds.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="default-time-limit" type="timeout" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The default time limit for a request, in seconds.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="authentication">
    <xs:annotation>
      <xs:documentation>The authentication mode for the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:NMTOKEN">
      <xs:enumeration value="application-level"/>
      <xs:enumeration value="basic"/>
      <xs:enumeration value="digest"/>
      <xs:enumeration value="digestbasic"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="authentication" type="authentication" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The authentication scheme to use for this server</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="user">
    <xs:annotation>
      <xs:documentation>A user ID.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="default-user" type="user" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The user used as the default user in application level authentication. Using the admin user as the default
      user is equivalent to turning security off.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="privilege-id">
    <xs:annotation>
      <xs:documentation>A privilege to restrict access to a server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="privilege" type="privilege-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The privilege restricting access to the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="pathname">
    <xs:annotation>
      <xs:documentation>It can be absolute or relative.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="path" type="pathname" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The path to a queery</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="text">
    <xs:annotation>
      <xs:documentation>A query/request text</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="request-text" type="text" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The request text.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="request-rewritten-text" type="text" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The rewritten request text.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="start-time">
    <xs:annotation>
      <xs:documentation>A Label version number.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:dateTime"/>
  </xs:simpleType>
  <xs:element name="start-time" type="start-time" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The time the operation started.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="trigger-depth">
    <xs:annotation>
      <xs:documentation>An integer number of trigger invocation.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="trigger-depth" type="trigger-depth" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The maximum depth of trigger invocation.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="user" type="user" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The unique ID of the user executing the request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="boolean">
    <xs:annotation>
      <xs:documentation>true or false</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>
  <xs:element name="update" type="boolean" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Specifies if this request an update (true for update requests).</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="request-id">
    <xs:annotation>
      <xs:documentation>An id for a request</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="request-id" type="request-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>A unique id for a request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="canceled" type="boolean" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Has this request been canceled.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="client-address">
    <xs:annotation>
      <xs:documentation>An address of a client</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:element name="client-address" type="client-address" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The network address of a client</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="profiling-allowed" type="boolean" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Is profiling allowed for this request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="profiling-enabled" type="boolean" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Is profiling currently enabled for this request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="debugging-allowed" type="boolean" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Is debugging allowed for this request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="debugging-status">
    <xs:annotation>
      <xs:documentation>A request's debugging status.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:element name="debugging-status" type="debugging-status" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The debugging status of the request.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="expr-id">
    <xs:annotation>
      <xs:documentation>An expression identifier.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedLong"/>
  </xs:simpleType>
  <xs:element name="expr-id" type="expr-id" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The expression id.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="where-stopped">
    <xs:annotation>
      <xs:documentation>Where in an expression a request is stopped.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="begin"/>
      <xs:enumeration value="end"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="where-stopped" type="where-stopped" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>Where the request is stopped.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="request-status" mixed="false" abstract="false">
    <xs:annotation>
      <xs:documentation>A set of request statuses.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element ref="request-id" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="server-id" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="host-id" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="canceled" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="modules" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="database" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="root" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="request-kind" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="request-text" minOccurs="1" maxOccurs="1"/>
      <xs:element minOccurs="0" ref="request-rewritten-text" maxOccurs="1"/>
      <xs:element ref="update" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="start-time" minOccurs="1" maxOccurs="1"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="client-address"/>
      <xs:element ref="user" minOccurs="1" maxOccurs="1"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="trigger-depth"/>
      <xs:element ref="expanded-tree-cache-hits" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="expanded-tree-cache-misses" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="profiling-allowed" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="profiling-enabled" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="debugging-allowed" minOccurs="1" maxOccurs="1"/>
      <xs:element ref="debugging-status" minOccurs="1" maxOccurs="1"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="expr-id"/>
      <xs:element maxOccurs="1" minOccurs="0" ref="where-stopped"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="request-status" type="request-status" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The request being processed by the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="request-statuses" mixed="false" abstract="false">
    <xs:annotation>
      <xs:documentation>A set of request statuses.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:choice maxOccurs="unbounded" minOccurs="0">
      <xs:element ref="request-status" minOccurs="1" maxOccurs="1"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="request-statuses" type="request-statuses" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The requests being processed by the server.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="queue-size">
    <xs:annotation>
      <xs:documentation>An integer number of tasks.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" fixed="false"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="queue-size" type="queue-size" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The number of tasks in the server queue.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="queue-limit" type="queue-size" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The maximum number size of the queue.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="socket-count">
    <xs:annotation>
      <xs:documentation>A number of sockets</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt"/>
  </xs:simpleType>
  <xs:element name="open-sockets" type="socket-count" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The number of open sockets</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name="count">
    <xs:annotation>
      <xs:documentation>A count.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:unsignedInt"/>
  </xs:simpleType>
  <xs:simpleType name="average">
    <xs:annotation>
      <xs:documentation>An average.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
    <xs:restriction base="xs:float"/>
  </xs:simpleType>
  <xs:element name="expanded-tree-cache-hits" type="count" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The number of hits on the expanded cache.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="expanded-tree-cache-misses" type="count" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The number of misses on the expanded cache.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="fs-program-cache-hits" type="count" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The number of hits on the filesystem program cache.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
  </xs:element>
  <xs:element name="fs-program-cache-misses" type="count" abstract="false" nillable="false">
    <xs:annotation>
      <xs:documentation>The number of misses on the filesystem program cache.</xs:documentation>
      <xs:appinfo/>
    </xs:annotation>
